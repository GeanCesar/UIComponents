<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressButton">
        <attr name="texto" format="string" />
        <attr name="icone" format="reference|integer" />
        <attr name="fundo" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="NumberSelector">
        <attr name="valor" format="integer" />
        <attr name="valor_minimo" format="integer" />
        <attr name="valor_maximo" format="integer" />
    </declare-styleable>

    <declare-styleable name="ValueButton">
        <attr name="texto_value_button" format="string"/>
        <attr name="valor_value_button" format="float" />
        <attr name="fundo_value_button" format="reference|color" />
    </declare-styleable>
</resources>